/*
 * Media queries for responsive design
 * These follow after primary styles so they will successfully override. 
 */
@media screen and (min-width: 1500px) {
	.l-margin {width:1400px;margin:0 auto;}
	#slides  {margin-right:0px;}
}
 
@media screen and (max-width: 1060px) {
	.l-margin {margin:0 30px;}
	.header .l-margin {margin:0;}
	#slides  {margin-right:-30px;}
}
@media screen and (max-width: 980px) {
	.header .header-map {display:none;}
}


@media screen and (max-width: 800px) {
	.l-margin {margin:0 20px;}
	.l-margin.mainnavigation  {margin:0;}

	#slides  {margin-right:0px;}
	.menu-title {float:none;width:auto;}
	.slides-container {margin:0;width:100%;}
	.menu-content {margin:0;width:100%;padding:0;}
	
	.verticalmenu {margin:0;width:2000%;float:none;display:block;}
	.mp-nav .verticalmenu {margin:0;}
	.verticalmenu .mainmenu {margin:0;padding:0 20px;overflow:hidden;border-bottom:1px solid #A8CEDB;}
	.verticalmenu .mainmenu li {float:left;}
	.verticalmenu .mainmenu li a {padding:15px;}
	.verticalmenu .mainmenu li {}
	.verticalmenu .mainmenu li + li {border-left:1px solid #A8CEDB;}
	.verticalmenu .servicemenu {margin:0;float:right;}
	.verticalmenu .servicemenu li {float:left;}
	
	.l-zero {left:0;}
	.l-base {width:2000%;}
	
	.footer .vcard .subzero {width:100%;}
	
	.header-phones {font-size:70%;}
}

